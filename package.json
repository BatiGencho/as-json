{
  "name": "json-as",
  "version": "0.1.0",
  "description": "JSON encoder/decoder for AssemblyScript",
  "types": "assembly/index.ts",
  "ascMain": "assembly/index.ts",
  "author": "JairusSW",
  "license": "MIT",
  "aspkg": {
    "type": "git",
    "platforms": [
      "wasm",
      "node",
      "web"
    ]
  },
  "scripts": {
    "build:transform": "npx tsc -w -p ./transform",
    "build:test": "asc assembly/test.ts --transform json-as/transform --target test --explicitStart",
    "test": "node --experimental-wasi-unstable-preview1 ./tests/test",
    "bench:build": "asc assembly/bench.ts --runtime stub --target bench --explicitStart",
    "bench": "node --experimental-wasi-unstable-preview1 ./bench/bench"
  },
  "devDependencies": {
    "@assemblyscript/loader": "^0.19.5",
    "@types/line-column": "^1.0.0",
    "as-console": "^6.0.0",
    "as-string-sink": "^0.2.2",
    "assemblyscript": "^0.19.5",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "as-wasi": "^0.4.5",
    "json-as": "aspkg/as-json",
    "line-column": "^1.0.2",
    "visitor-as": "^0.6.0"
  },
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aspkg/as-json.git"
  },
  "keywords": [
    "assemblyscript",
    "json",
    "serialize",
    "deserialize"
  ],
  "bugs": {
    "url": "https://github.com/aspkg/as-json/issues"
  },
  "homepage": "https://github.com/aspkg/as-json#readme"
}
