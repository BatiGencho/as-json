{
  "name": "json-as",
  "version": "0.1.4",
  "description": "JSON encoder/decoder for AssemblyScript",
  "types": "assembly/index.ts",
  "ascMain": "assembly/index.ts",
  "author": "JairusSW",
  "license": "MIT",
  "aspkg": {
    "type": "git",
    "platforms": [
      "wasm",
      "node",
      "web"
    ]
  },
  "scripts": {
    "dynamic:transform": "tsc -w -p ./dynamic/transform",
    "dynamic:build:test": "asc ./dynamic/test.ts --transform ./dynamic/transform/lib --target dynamic:test --explicitStart",
    "dynamic:build:bench": "asc ./dynamic/bench.ts --transform ./dynamic/transform/lib --target dynamic:bench --explicitStart",
    "dynamic:test": "wasmtime ./dynamic/test/output/test.wasm",
    "dynamic:bench": "wasmtime ./dynamic/bench/output/bench.wasm",
    "build:transform": "tsc -w -p ./transform",
    "build:test": "asc assembly/test.ts --transform ./transform --target test --explicitStart",
    "test": "node --experimental-wasi-unstable-preview1 ./tests/test",
    "bench:build": "asc assembly/bench.ts --transform ./transform --target bench --explicitStart",
    "bench:node": "node --experimental-wasi-unstable-preview1 ./bench/bench",
    "bench:wasmtime": "wasmtime ./bench/output/bench.wasm",
    "bench:lunatic": "lunatic ./bench/output/bench.wasm"
  },
  "devDependencies": {
    "@assemblyscript/loader": "^0.19.5",
    "@types/line-column": "^1.0.0",
    "as-console": "^6.0.0",
    "as-string-sink": "^0.2.2",
    "as-variant": "^0.2.1",
    "as-wasi": "^0.4.6",
    "assemblyscript": "^0.19.5",
    "assemblyscript-json": "^1.1.0",
    "kati": "^0.5.1"
  },
  "dependencies": {
    "line-column": "^1.0.2",
    "typescript": "^4.3.5",
    "visitor-as": "^0.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aspkg/as-json.git"
  },
  "keywords": [
    "assemblyscript",
    "json",
    "serialize",
    "deserialize"
  ],
  "bugs": {
    "url": "https://github.com/aspkg/as-json/issues"
  },
  "homepage": "https://github.com/aspkg/as-json#readme"
}
